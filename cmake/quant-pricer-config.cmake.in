@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(_quant_pricer_has_openmp "@QUANT_PRICER_HAS_OPENMP@")
if(_quant_pricer_has_openmp)
  find_dependency(OpenMP)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/quant-pricer-targets.cmake")

if(TARGET quant_pricer::quant_pricer AND NOT TARGET quant_pricer)
  add_library(quant_pricer INTERFACE IMPORTED)
  target_link_libraries(quant_pricer INTERFACE quant_pricer::quant_pricer)
endif()

check_required_components(quant-pricer)
